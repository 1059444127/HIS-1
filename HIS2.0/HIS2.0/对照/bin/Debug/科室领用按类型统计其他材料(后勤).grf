			ConnectionString='Provider=SQLOLEDB.1;Password=DNZX_admin;Persist Security Info=True;User ID=sa;Initial Catalog=trasen;Data Source=192.168.100.4;Use Procedure for Prepare=1;Auto Translate=True;Packet Size=4096;Workstation ID=PC367-20130830;Use Encryption for Data=False;Tag with column collation when possible=False'
			QuerySQL='create table #tab      \r\n(      \r\n typeID int ,      \r\n 支出名称 varchar(50),      \r\n 往来单位  varchar(50),      \r\n wzfl int,      \r\n 物资类型 varchar(50),      \r\n 金额 decimal(15,2),      \r\n 对应名称 varchar(50),      \r\n 对应ID int      \r\n       \r\n) \r\n\r\n\r\n\r\ninsert into #tab \r\nselect  isnull(type,2) typeID , (case when j.mc is not null then j.mc else '后勤支出' end)  支出名称 ,      \r\ndbo.fun_wz_wldw(a.ywlx,wldw) 往来单位,wzfl ,dbo.fun_wz_flmc(wzfl) 物资类型,      \r\n-1*sum(DBO.FUN_WZ_DRT(A.YWLX,jhje))  金额  ,       \r\n(case  when g.DyName Is not null then  g.DyName  else '未知' end) 对应名称  ,      \r\n(case  when g.id Is not null then g.id  else null end) 对应ID          \r\n  from wz_dj a inner join wz_djmx b on a.id=b.djid                                      \r\n   left join (SELECT DISTINCT deptid,[type],dept_ck FROM wz_dept WHERE dept_ck=102 and bscbz=0 AND bghdw=0) c on a.wldw=c.deptid                                                       \r\n   inner join vi_wz_cd d on b.wzid=d.wzid                                    \r\n   inner join wz_wzlx e on d.wzlx=e.id                                    \r\n   inner join wz_wzzlx f on d.wzfl=f.id          \r\n   left join wz_wzzlxDy g on f.id=g.wzlxid         \r\n      left join wz_type j on c.type=j.ID                                 \r\n  where                                   \r\n    a.ywlx in ('003','006')                                    \r\nand bshbz =1 and a.bscbz=0                                    \r\nand a.deptid in (102)                                                     \r\nand a.shrq>='2014-02-01 00:00:01' \r\nand a.shrq<='2014-02-28 23:59:59'                         \r\nand ((d.wzlx=48 and 48<>0 ) or 48=0)                             \r\nand a.deptid=       102                                                       \r\n   group by type,wldw,wzfl,e.hhbm,f.hhbm,a.YWLX    ,j.mc   ,   g.DyName, g.id                                                           \r\n    UNION ALL                                                    \r\n  select  isnull(type,2) typeID ,(case when j.mc is not null then j.mc else '后勤支出' end)  支出名称 ,      \r\ndbo.fun_wz_wldw(a.ywlx,wldw) 往来单位,wzfl ,dbo.fun_wz_flmc(wzfl) 物资类型,      \r\n-1*sum(DBO.FUN_WZ_DRT(A.YWLX,jhje))  金额  ,       \r\n(case  when g.DyName Is not null then  g.DyName  else '未知' end) 对应名称  ,      \r\n(case  when g.id Is not null then g.id  else 0 end) 对应ID        \r\n   from wz_dj a inner join wz_djmx b on a.id=b.djid                                      \r\n   left join  (SELECT DISTINCT deptid,[type],dept_ck FROM wz_dept WHERE dept_ck=102 and bscbz=0 AND bghdw=1) c on a.wldw=c.deptid                                         \r\n      inner join vi_wz_cd d on b.wzid=d.wzid                                    \r\n      inner join wz_wzlx e on d.wzlx=e.id                                    \r\n      inner join wz_wzzlx f on d.wzfl=f.id       \r\n      left join wz_wzzlxDy g on f.id=g.wzlxid             \r\n         left join wz_type j on c.type=j.ID                                \r\n    where a.ywlx in('007') and bshbz =1 and a.bscbz=0                                                    \r\n   and a.deptid in (102)                                \r\n  and a.shrq>='2014-02-01 00:00:01' and a.shrq<='2014-02-28 23:59:59'                         \r\n    and ((d.wzlx=48 and 48<>0 ) or 48=0)                                \r\n    and a.deptid=       102               \r\n   group by type,wldw,wzfl,e.hhbm,f.hhbm,a.YWLX    ,j.mc      ,   g.DyName, g.id    order by  typeID  \r\n\r\n   declare @sql varchar(8000)      \r\nset @sql = ' select  typeID ,  支出名称 ,往来单位,wzfl , 物资类型  '      \r\nselect @sql = @sql + ' , sum(case 对应名称 when ''' + 对应名称+ ''' then 金额 else null end) [' +对应名称 + ']'      \r\nfrom (select distinct 对应名称 from #tab) as a       \r\nset @sql = @sql + ' , 对应名称 ,对应ID from #tab group by  typeID ,  支出名称 ,往来单位,wzfl , 物资类型,金额, 对应名称  ,对应ID  '      \r\nexec(@sql)'
